        ALARM {
            if #PARAM 1 of #METHOD java.net.URLClassLoader.<init>(java.net.URL[]) is_from @SRC_INPUT
        }
        /* 로드된 클래스가 안전한지 검사하는 Method 로 입력이 필요함 */
        EXCLUDE {
            if #TAINT_INVOKED_BY #METHOD java.lang.Class.getName() equals #TAINT_INVOKED_BY #METHOD java.lang.Class.getSimpleName()
            if #TAINT_VAL pass #METHOD java.lang.Integer.parseInt(java.lang.String, java.lang.Integer)
            if #TAINT_VAL pass #METHOD java.lang.Integer.parseInt(java.lang.String)
            if #TAINT_VAL pass #METHOD java.lang.Long.parseLong(java.lang.String, java.lang.Integer)
            if #TAINT_VAL pass #METHOD java.lang.Long.parseLong(java.lang.String)
            if #TAINT_VAL pass #METHOD java.lang.Double.parseDouble(java.lang.String)
            if #TAINT_VAL pass #METHOD java.lang.Float.parseFloat(java.lang.String)
            if #TAINT_VAL pass #METHOD java.lang.String.length()
        }
        PROPAGATOR {
            #PARAM 1 of #METHOD java.net.URL.<init>(java.lang.String) to #BASE
        }
    }
    @description {
        원격으로부터 소스코드 또는 실행 파일을 무결성 검사 없이 다운로드 받고, 이를 실행하는 제품들이 종종 존재. 이는 호스트 서버의 변조, DNS 스푸핑(Spoofing) 또는 전송시의 코드 변조 등의 방법을 이용하여 공격자가 악의적인 코드를 실행할 수 있도록 한다.
    }
}

