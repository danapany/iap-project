            if #TAINT_VAL pass #METHOD java.lang.Long.parseLong(java.lang.String)
            if #TAINT_VAL pass #METHOD java.lang.Double.parseDouble(java.lang.String)
            if #TAINT_VAL pass #METHOD java.lang.Float.parseFloat(java.lang.String)
            if #TAINT_VAL pass #METHOD java.lang.String.length()
            if #TAINT_VAL pass #METHOD java.lang.String.replaceAll(java.lang.String, java.lang.String)
            if #TAINT_VAL pass #METHOD org.apache.commons.lang.StringEscapeUtils.escapeHtml(java.lang.String)
            if #TAINT_VAL pass #METHOD org.apache.commons.text.StringEscapeUtils.escapeHtml3(java.lang.String)
            if #TAINT_VAL pass #METHOD org.apache.commons.text.StringEscapeUtils.escapeHtml4(java.lang.String)
