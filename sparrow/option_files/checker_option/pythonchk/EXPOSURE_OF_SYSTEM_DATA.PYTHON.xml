<?xml version="1.0" encoding="UTF-8"?><option>
    <category name="EXPOSURE_OF_SYSTEM_DATA.PYTHON">
        <item name="taint_source" type="string" required="true" variable="true" regexp="true">
            <value>return of fqn(reg("traceback.format_.*"))</value>
            <value>return of fqn(reg("traceback.extract_.*"))</value>
            <value>return of fqn("os.getenv")</value>
        </item>
        <item name="taint_sink" type="string" required="true" variable="true" regexp="true">
            <value>param(*) of fqn("django.http.HttpResponse.write")</value>
            <value>param(*) of fqn("django.http.HttpResponse.writelines")</value>
            <value>param(*) of fqn("django.http.HttpResponse")</value>
            <value>param(*) of fqn("flask.make_response")</value>
            <value>param(*) of fqn("flask.Response")</value>
            <value>param(*) of fqn("print")</value>
        </item>
        <item name="taint_propagator" type="string" required="false" variable="true" regexp="true">
            <!--propagate string-->
<value>operand(*) of "+" to return</value>
<value>param(*) of name("join") to return</value>
<value>operand(2) of "%" to return</value>
<!-- ___ -->
        </item>
        <item name="taint_filter" type="string" required="false" variable="true" regexp="true">
        </item>
        <item name="taint_filter_function_fqn" type="string" required="false" variable="true" regexp="true">
        </item>
        <item name="forbidden_function" type="string" required="false" variable="true" regexp="true">
            <value>traceback.print_.*</value>
        </item>
    </category>
</option>

