<?xml version="1.0" encoding="UTF-8"?><option>
    <category name="MEMCACHE_INJECTION.PYTHON">
        <item name="taint_source" type="string" required="true" variable="true" regexp="true">
            <!--src general-->
<value>return of fqn("read")</value>
<value>return of fqn("open")</value>
<value>return of fqn("os.open")</value>
<value>return of fqn("os.getenv")</value>
<!-- ___ -->
            <!--src django-->
<value>variable of fqn("django.http.HttpRequest.GET")</value>
<value>return of fqn("django.http.HttpRequest.field")</value>
<value>variable of fqn(reg(".*\.GET"))</value>
<value>return of fqn(reg(".*\.field"))</value>
<!-- ___ -->
        </item>
        <item name="taint_sink" type="string" required="true" variable="true" regexp="true">
            <value>param(1) of name("set") if base == fqn("django.core.cache") or base == fqn("django.core.caches") or base == name(reg(".*cache.*"))</value>
        </item>
        <item name="taint_propagator" type="string" required="false" variable="true" regexp="true">
            <value>base of name("read") to return</value>
            <!--propagate string-->
<value>operand(*) of "+" to return</value>
<value>param(*) of name("join") to return</value>
<value>operand(2) of "%" to return</value>
<!-- ___ -->
        </item>
        <item name="taint_filter" type="string" required="false" variable="true" regexp="true">
            <!--filter string-->
<value>operand(*) of "in"</value>
<value>operand(*) of "not in"</value>
<value>base of name("replace")</value>
<value>base of name("translate")</value>
<value>return of name("filter")</value>
<!-- ___ -->
        </item>
        <item name="taint_filter_function_fqn" type="string" required="false" variable="true" regexp="true">
            <value>sparrow.my.filter.utils.is_safe()</value>
        </item>
    </category>
</option>

