<?xml version="1.0" encoding="utf-8"?>
<option>
    <category name="SQL_INJECTION_MYSQL.JS">
        <item name="taint_source" type="string" required="false" variable="true" regexp="true">
            <!-- using custom source !-->
        </item>
        <item name="taint_sink" type="string" required="true" variable="true" regexp="true">
            <value>param(1) of fqn(reg("__sparrow_required__mysql\..*\.query"))</value>
            <value>param(1) of fqn(reg("__sparrow_required__mysql2\..*\.query"))</value>
            <value>param(1) of fqn(reg("__sparrow_required__mysql\..*\.pool"))</value>
            <value>param(1) of fqn(reg("__sparrow_required__mysql2\..*\.pool"))</value>
        </item>
        <item name="taint_propagator" type="string" required="false" variable="true" regexp="true">
            <value>operand(*) of name("+") to return</value>
            <value>return of name("=") to return</value>
            <value>return of name(".") to return</value>
        </item>
        <item name="taint_filter" type="string" required="false" variable="true" regexp="true">
            <value>return of fqn(reg("__sparrow_required__mysql\..*\.escape"))</value>
            <value>return of fqn(reg("__sparrow_required__mysql\..*\.escapeId"))</value>
            <value>return of fqn(reg("__sparrow_required__mysql\.escape"))</value>
            <value>return of fqn(reg("__sparrow_required__mysql\.escapeId"))</value>
        </item>
        <item name="taint_filter_function_fqn" type="string" required="false" variable="true" regexp="true">
        </item>
    </category>
</option>

